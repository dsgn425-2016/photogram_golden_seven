<h1>Edit photo</h1>


<form action="http://localhost:3000/update_photo" >
  <label >Caption:</label>
  <input type="text" name="the_caption" value="<%= @photo_caption %>">

  <label>Image URL:</label>
  <input type="text" name="the_source" value="<%= @photo_source %>">

  <button>Update Photo</button>
</form>
