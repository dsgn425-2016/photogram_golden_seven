<h1>Edit photo</h1>

<form action="http://localhost:3000/update_photo/<%= @photo.id %>">


  <div>
  <label >Caption:</label>
  <input type="text" name="new_caption" value="<%= @photo.caption %>">
</div>
  <div>
    <label >Image URL:</label>
    <input type="text" name="new_source" value="<%= @photo.source %>">
  </div>
  <div >
    <button>Update Photo</button>
  </div>

</form>
