<h1>Edit photo</h1>
<form action="/update_photo/<%= @photo.id %>">
  <label for="caption">Caption:</label>
  <input type="text" id="caption" name="new_caption" value="<%=  @photo.caption %>">

  <div><label for="image_url">Image URL:</label>
    <input type="text" id="image_url" name="new_source" value="<%=  @photo.source %>">
  </div>

    <button>Update Photo</button>
  </form>
